{
  "name": "h5p-generate-configs",
  "version": "0.0.2",
  "description": "h5p-generate-configs CLI",
  "private": false,
  "types": "build/types/types.d.ts",
  "bin": {
    "h5p-generate-configs": "bin/h5p-generate-configs"
  },
  "scripts": {
    "clean-build": "rm -rf ./build",
    "compile": "tsc -p .",
    "copy-templates": "copyfiles ./src/templates/* ./build/templates",
    "build": "npm run clean-build && npm run compile && npm run copy-templates",
    "prepublishOnly": "npm run build",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage",
    "demo:semantics": "h5p-generate-configs -s demo/semantics.ts --semanticsOut demo/semantics.json -t demo/TranslationKey.ts"
  },
  "files": [
    "build",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "gluegun": "latest",
    "h5p-types": "github:boyum/h5p-types",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^17.0.27",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
